{
  "name": "fum",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "script": "tsx",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "hardhat test",
    "coverage": "hardhat coverage",
    "test-contracts": "bash -c './sync-contracts.sh && cd ~/code/fum_testing && npx hardhat test'",
    "test-contracts:coverage": "bash -c './sync-contracts.sh && cd ~/code/fum_testing && npx hardhat coverage'",
    "hardhat-node": "bash -c 'cd ~/code/fum_testing && npx hardhat node'",
    "deploy-setup": "bash -c './sync-contracts.sh && cd ~/code/fum_testing && npx hardhat compile && cd ~/code/fum && npm run script scripts/extract-bytecode.js'",
    "seed-localhost": "bash -c 'npm run script scripts/deploy.js --network=localhost && npm run script scripts/create-test-vault.js --network=localhost && npm run script scripts/seed.js --network=localhost'"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.0",
    "@uniswap/v3-sdk": "^3.24.1",
    "@web3modal/ethereum": "^2.7.1",
    "bootstrap": "^5.3.3",
    "ethers": "^6.13.5",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@openzeppelin/contracts": "^5.2.0",
    "@typechain/ethers-v6": "^0.4.0",
    "@typechain/hardhat": "^8.0.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "chai": "^5.2.0",
    "dotenv": "^16.4.7",
    "hardhat": "^2.22.19",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.8.2",
    "tsx": "^4.19.3",
    "typechain": "^8.2.0"
  }
